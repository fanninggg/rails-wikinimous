<h1>Wikinimous</h1>
<p><% @articles.each do |article| %></p>
  <%= link_to article.title, article_path(article), class: "links" %>
  <%= link_to 'Delete', article_path(article), method: :delete, data: {confirm: "Are you sure?"}, class: "link"%>
  <p></p>
  <%= article.content%>
<% end %>
<p></p>
<%= link_to ' + Create a new article', new_article_path, class: "link"%>
